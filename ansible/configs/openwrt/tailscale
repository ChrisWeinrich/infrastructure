# Tailscale UCI configuration (managed by Ansible).
config settings 'settings'
  option enabled '1'
  option log_stderr '1'
  option log_stdout '1'
  option port '41641'
  option state_file '/etc/tailscale/tailscaled.state'
  # Auth key is stored in dcli at openwrt/mt6000/tailscale/auth_key and is
  # injected by Ansible during `tailscale up`.
  # Initial US1 route: 192.168.8.135/32 (US2 expands to 192.168.8.0/24).
  # Advertised routes are controlled via openwrt_tailscale_routes.
